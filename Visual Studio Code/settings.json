{
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontSize": 18,
  "editor.lineHeight": 25,
  "editor.letterSpacing": 0.5,
  "files.trimTrailingWhitespace": true,
  "editor.fontWeight": "400",
  "editor.renderWhitespace": "all",
  "terminal.integrated.fontFamily": "JetBrains Mono",
  "terminal.integrated.fontSize": 18,
  "workbench.startupEditor": "newUntitledFile",
  "editor.rulers": [
    120
  ],
  "editor.tabSize": 2,
  "terminal.integrated.scrollback": 100000,
  "editor.cursorBlinking": "phase",
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "ruby.intellisense": "rubyLocate",
  "eslint.alwaysShowStatus": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue",
    "pug"
  ],
  "window.zoomLevel": 0,
  "files.maxMemoryForLargeFilesMB": 10000,
  "eslint.packageManager": "yarn",

  "breadcrumbs.enabled": true,
  "editor.fontLigatures": true,
  "workbench.fontAliasing": "antialiased",
  "ruby.rubocop.executePath": "/Users/k1r8r0wn/.rbenv/shims/",
  "diffEditor.ignoreTrimWhitespace": true,
  "workbench.preferredLightColorTheme": "GitHub Light",
  "workbench.preferredDarkColorTheme": "Darcula",
  "workbench.colorTheme": "Darcula",
  "files.defaultLanguage": "markdown",
  "editor.renderIndentGuides": false,
  "editor.wordWrap": "off",
  "diffEditor.wordWrap": "off",
  "blockman.n32BlackListOfFileFormats": "plaintext, markdown, ruby, coffeescript",
  "blockman.n04ColorComboPreset": "Classic Light (Gradients)",
  "security.workspace.trust.untrustedFiles": "open",
  "tabnine.experimentalAutoImports": true,
  "emmet.excludeLanguages": [
    "javascript",
    "markdown"
  ],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "emmet.preferences": {},
  "liveServer.settings.donotShowInfoMsg": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.suggestionActions.enabled": false,
  "editor.inlayHints.enabled": false,
  "window.autoDetectColorScheme": true,
  "terminal.integrated.cursorStyle": "line",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "terminal.external.osxExec": "iTerm2.app"
}
