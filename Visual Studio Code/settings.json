{
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontSize": 18,
  "editor.lineHeight": 25,
  "editor.letterSpacing": 0.5,
  "files.trimTrailingWhitespace": true,
  "editor.fontWeight": "400",
  "editor.renderWhitespace": "all",
  "terminal.integrated.fontFamily": "JetBrains Mono",
  "terminal.integrated.fontSize": 18,
  "workbench.startupEditor": "newUntitledFile",
  "editor.rulers": [
    140
  ],
  "editor.tabSize": 2,
  "terminal.integrated.scrollback": 100000,
  "editor.cursorBlinking": "phase",
  "ruby.intellisense": "rubyLocate",
  "eslint.alwaysShowStatus": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue",
    "pug"
  ],
  "eslint.format.enable": true,
  "window.zoomLevel": 0,
  "files.maxMemoryForLargeFilesMB": 10000,
  "breadcrumbs.enabled": true,
  "editor.fontLigatures": true,
  "workbench.fontAliasing": "antialiased",
  "diffEditor.ignoreTrimWhitespace": true,
  "workbench.preferredLightColorTheme": "GitHub Light",
  "workbench.preferredDarkColorTheme": "Darcula",
  "workbench.colorTheme": "Darcula",
  "files.defaultLanguage": "markdown",
  "editor.wordWrap": "off",
  "diffEditor.wordWrap": "off",
  "security.workspace.trust.untrustedFiles": "open",
  "emmet.excludeLanguages": [
    "javascript",
    "markdown"
  ],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "emmet.preferences": {},
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "javascript.suggestionActions.enabled": false,
  "editor.inlayHints.enabled": "off",
  "window.autoDetectColorScheme": true,
  "terminal.integrated.cursorStyle": "line",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "terminal.external.osxExec": "iTerm2.app",
  "gitlens.currentLine.enabled": false,
  "gitlens.currentLine.pullRequests.enabled": false,
  "gitlens.currentLine.scrollable": false,
  "gitlens.views.fileHistory.files.compact": false,
  "gitlens.views.fileHistory.avatars": false,
  "gitlens.views.lineHistory.avatars": false,
  "gitlens.views.contributors.pullRequests.enabled": false,
  "gitlens.views.contributors.pullRequests.showForCommits": false,
  "gitlens.views.contributors.files.compact": false,
  "gitlens.views.contributors.avatars": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "workbench.preferredHighContrastColorTheme": "Darcula",
  "editor.guides.indentation": false,
  "editor.codeActionsOnSave": {
  },
  "editor.language.colorizedBracketPairs": [],
  "[ruby]": {
    "editor.defaultFormatter": "misogi.ruby-rubocop"
  },
  "json.schemas": [],
  "code-runner.enableAppInsights": false,
  "telemetry.telemetryLevel": "off",
  "auto-close-tag.excludedTags": [
    "area",
    "base",
    "br",
    "col",
    "command",
    "embed",
    "hr",
    "img",
    "input",
    "keygen",
    "link",
    "meta",
    "param",
    "source",
    "track",
    "wbr"
  ],
  "gitlens.gitCommands.skipConfirmations": [
    "fetch:command",
    "stash-push:command",
    "switch:command",
    "branch-create:command"
  ],
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "[scss]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint"
  },
  "stylelint.snippet": [
    "css",
    "less",
    "postcss",
    "scss"
  ],
  "stylelint.validate": [
    "css",
    "less",
    "postcss",
    "scss"
  ],
  "[javascriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  }
}
