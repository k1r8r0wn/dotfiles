{
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontSize": 18,
  "editor.lineHeight": 25,
  "editor.letterSpacing": 0.5,
  "files.trimTrailingWhitespace": true,
  "editor.fontWeight": "400",
  "editor.renderWhitespace": "all",
  "terminal.integrated.fontFamily": "JetBrains Mono",
  "terminal.integrated.fontSize": 18,
  "workbench.startupEditor": "newUntitledFile",
  "editor.rulers": [
    140
  ],
  "editor.tabSize": 2,
  "terminal.integrated.scrollback": 100000,
  "editor.cursorBlinking": "phase",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue",
    "pug"
  ],
  "eslint.format.enable": true,
  "breadcrumbs.enabled": true,
  "editor.fontLigatures": true,
  "workbench.fontAliasing": "antialiased",
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.preferredLightColorTheme": "Default Light Modern",
  "workbench.preferredDarkColorTheme": "Default Dark Modern",
  "files.defaultLanguage": "markdown",
  "editor.wordWrap": "off",
  "diffEditor.wordWrap": "off",
  "security.workspace.trust.untrustedFiles": "open",
  "emmet.excludeLanguages": [
    "javascript",
    "markdown"
  ],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html",
    "html.erb": "html"
  },
  "emmet.preferences": {},
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "javascript.suggestionActions.enabled": false,
  "editor.inlayHints.enabled": "off",
  "window.autoDetectColorScheme": true,
  "terminal.integrated.cursorStyle": "line",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },

  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.guides.indentation": false,
  "editor.language.colorizedBracketPairs": [],
  "[ruby]": {
    "editor.defaultFormatter": "Shopify.ruby-lsp",
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.rulers": [
      120
    ],
    "editor.semanticHighlighting.enabled": true
  },
  "json.schemas": [],
  "code-runner.enableAppInsights": false,
  "telemetry.telemetryLevel": "off",
  "auto-close-tag.excludedTags": [
    "area",
    "base",
    "br",
    "col",
    "command",
    "embed",
    "hr",
    "img",
    "input",
    "keygen",
    "link",
    "meta",
    "param",
    "source",
    "track",
    "wbr"
  ],
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "[scss]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint"
  },
  "stylelint.snippet": [
    "css",
    "less",
    "postcss",
    "scss"
  ],
  "stylelint.validate": [
    "css",
    "less",
    "postcss",
    "scss"
  ],
  "[javascriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "git.autofetch": true,
  "solargraph.definitions": false, // This actualy enables go to on click
  "solargraph.folding": false,
  "solargraph.hover": false,
  "ruby.useLanguageServer": true,
  "ruby.intellisense": "rubyLocate",
  "todo-tree.highlights.customHighlight": {
    "TODO": {
        "foreground": "#ffbe31",
        "iconColour": "#ffbe31",
        "gutterIcon": true
    },
    "HACK": {
        "foreground": "#ec210f",
        "iconColour": "#ec210f",
        "gutterIcon": true
    }
  },
  "remote.extensionKind": {
    "pub.name": [
      "ui"
    ]
  },
  "workbench.preferredHighContrastLightColorTheme": "Solarized Light",
  "code-runner.executorMapByFileExtension": {
    ".vb": "cd $dir && vbc /nologo $fileName && $dir$fileNameWithoutExt",
    ".vbs": "cscript //Nologo",
    ".scala": "scala",
    ".jl": "julia",
    ".cr": "crystal",
    ".ml": "ocaml",
    ".zig": "zig run",
    ".exs": "elixir",
    ".hx": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
    ".rkt": "racket",
    ".scm": "csi -script",
    ".ahk": "autohotkey",
    ".au3": "autoit3",
    ".kt": "cd $dir && kotlinc $fileName -include-runtime -d $fileNameWithoutExt.jar && java -jar $fileNameWithoutExt.jar",
    ".kts": "kotlinc -script",
    ".dart": "dart",
    ".pas": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
    ".pp": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
    ".d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
    ".hs": "runhaskell",
    ".nim": "nim compile --verbosity:0 --hints:off --run",
    ".csproj": "dotnet run --project",
    ".fsproj": "dotnet run --project",
    ".lisp": "sbcl --script",
    ".kit": "kitc --run",
    ".v": "v run",
    ".vsh": "v run",
    ".sass": "sass --style expanded",
    ".cu": "cd $dir && nvcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    ".ring": "ring",
    ".sml": "cd $dir && sml $fileName"
  },
  "http.proxyAuthorization": null,
  "solargraph.diagnostics": true,
  "solargraph.transport": "stdio",
  "database-client.showProcedure": false,
  "database-client.showFunction": false,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "rubyLsp.yjit": false,
  "markdown.preview.doubleClickToSwitchToEditor": false,
  "markdown.preview.fontFamily": "SF Mono",
  "workbench.activityBar.location": "top",
  "explorer.confirmDelete": false,
}
